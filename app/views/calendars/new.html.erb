<h1>(自分の題名)</h1>
<h3>新規投稿ページ</h3>
<%= form_for @calendar do |f| %>


  <div class="field">
    <%= f.label :科目・部活 %>
    <%= f.text_field :subject, :size => 30 %>
  </div>

<div class="field">
    <%= f.label :課題 %>
    <%= f.text_field :homework, :size => 30 %>
</div>



  <%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る", calendars_path %>